{
    "name": "phpais/ai-plugin",
    "description": "PHP AI 插件，整合文心、千问、火山、deepseek、混元、智谱清言、月之暗面等AI模型接口，支持ThinkPHP和Laravel框架",
    "type": "library",
    "version": "1.0.0",
    "keywords": ["ai", "php", "thinkphp", "laravel", "wenxin", "qianwen", "volcano", "deepseek"],
    "homepage": "https://github.com/phpais/ai-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "PHPais Team",
            "email": "team@phpais.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "guzzlehttp/guzzle": "^7.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Phpais\\AiPlugin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phpais\\AiPlugin\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Phpais\\AiPlugin\\Laravel\\AiPluginServiceProvider"
            ],
            "aliases": {
                "AI": "Phpais\\AiPlugin\\Laravel\\Facades\\AI"
            }
        },
        "thinkphp": {
            "services": [
                "Phpais\\AiPlugin\\ThinkPHP\\Service"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}